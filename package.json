  "homepage": "https://courseography.cdf.toronto.edu",
  "dependencies": {
    "babel-core": "^5.1.13",
    "babel-loader": "^5.0.0",
    "fb": "^0.7.0",
    "jest-cli": "^0.4.0",
    "jquery": "^2.1.3",
    "jquery-ui": "^1.10.5",
    "lodash": "^3.7.0",
    "react": "^0.13.2",
    "webpack": "^1.8.9"
  },
  "devDependencies": {
